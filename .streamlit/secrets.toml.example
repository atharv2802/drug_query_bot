# Streamlit Secrets Configuration
# Copy this file to secrets.toml and fill in your actual values
#
# DATABASE_URL can now be set via:
#   - CLI argument (--db_url)
#   - Environment variable (DATABASE_URL)
#   - Streamlit secrets (recommended for Streamlit Cloud)
#
# If using Streamlit locally or on Streamlit Cloud, set here:
DATABASE_URL = "postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres"

# OpenRouter API Key
OPENROUTER_API_KEY = "sk-or-v1-your-api-key-here"

# Supabase Configuration (optional, for direct Supabase client usage)
SUPABASE_URL = "https://your-project.supabase.co"
SUPABASE_KEY = "your-supabase-anon-key"
